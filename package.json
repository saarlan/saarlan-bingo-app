    "vue-router": "^4.4.0",
    "vue3-toastify": "^0.2.1",
    "vue3-ts-jsoneditor": "^2.11.2",
    "vuetify": "^3.5.8"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@import-meta-env/typescript": "^0.3.3",
    "@types/deep-equal": "^1.0.4",
