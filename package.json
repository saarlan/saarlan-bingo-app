  "scripts": {
    "makeenvdef": "import-meta-env-typescript -x .env.example.public -o src",
    "predev": "yarn makeenvdef",
    "dev": "vite --host --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "prestart": "import-meta-env -e .env.local -x .env.example.public",
    "start": "vite preview --host --port 3000",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "push": "./scripts/push-to-docker.sh",
    "deploy": "git checkout deploy && git merge main && git push && git checkout main",
    "deploy:patch": "git checkout main && git merge dev && git push && npm version patch && yarn deploy && git push && git checkout dev && git merge main && git push",
    "deploy:minor": "git checkout main && git merge dev && git push && npm version minor && yarn deploy && git push && git checkout dev && git merge main && git push",
    "deploy:major": "git checkout main && git merge dev && git push && npm version major && yarn deploy && git push && git checkout dev && git merge main && git push"
  },
  "dependencies": {
